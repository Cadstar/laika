<% patient_form_for([@patient, @insurance_provider_patient],
                   :update => "insurance_provider_patient#{@insurance_provider_patient.id}") do |f| -%>
<table>
  <tr class="<%= cycle("darkzebra", "lightzebra")%>">
    <th colspan="2" class="lighttext">Insurance Provider Patient</th>
  </tr>
  <%= f.text_field :member_id %>
  <%= f.calendar_date_select :start_coverage_date %>
  <%= f.calendar_date_select :end_coverage_date %>
  <%= f.person_name_fields %>
  <%= f.calendar_date_select :date_of_birth %>
  <%= f.address_fields %>
  <%= f.telecom_fields %>
  <%= f.submit "Save" %>
<table>
<% end %>
